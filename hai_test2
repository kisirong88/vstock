<script type="text/javascript">

    function ClearSearchBox(obj) {
        if (obj.value == 'Enter Symbols or Keywords')
            obj.value = '';
    }    
</script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#ctrlBoxSearch_txtKeyword').autocomplete("/vsSearchBox.ashx",
            {
                language: "en",
                autoFill: false,
                selectFirst: false,
                matchContains: true,
                width: 350,
                scroll: false,
                delay: 400,
                minChars: 1,
                cacheLength: 20,
                max: 10,
                formatItem: function (row, i, max) {
                    return row[0] + " " + row[1];
                },
                formatMatch: function (row, i, max) {
                    return row[0] + " " + row[1];
                },
                formatResult: function (row) {
                    return row[0];
                }

            }).result(function (event, row, formatted) {
                if (row) {
                    location.href = row[2];
                }
                /*else {
                    window.open('http://search.vietstock.vn/' + 'Result.aspx?lg=en&r=0&key=' + txtSearch.value + '&p=1&rdo=1', '_blank');
                }*/
            });
        $('#ctrlBoxSearch_txtKeyword').keypress(function (event) {
            if (event.keyCode == '13') {
                window.location.href = "http://en.vietstock.vn/Search.aspx?key=" + $(this).val();
                return false;
            }
        });
    });
</script>
