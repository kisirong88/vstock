            <script type="text/javascript">
                function getStockDataTop2() {
                    var currentTime = new Date();
                    var fullDate = currentTime.getTime();
                    var urlajax = "/Handler.ashx?u=http://en.vietstock.vn/vsChart.ashx?typeDo=getStockData&time=" + fullDate;
                    $.ajax({
                        url: urlajax,
                        type: "GET",
                        dataType: "json",
                        success: function (data) {
                            for (var i = 0; i < data.length; i++) {
                                /*Top Index*/
                                if (parseFloat(data[i].Change) == 0) {
                                    $("#TOP_PRICE_" + (i + 1)).addClass("CPause");
                                    $("#TOP_CHANGE_" + (i + 1)).addClass("CPause");
                                }
                                else if (parseFloat(data[i].Change) > 0) {
                                    $("#TOP_PRICE_" + (i + 1)).addClass("CUp");
                                    $("#TOP_CHANGE_" + (i + 1)).addClass("CUp");
                                }
                                else {
                                    $("#TOP_PRICE_" + (i + 1)).addClass("CDown");
                                    $("#TOP_CHANGE_" + (i + 1)).addClass("CDown");
                                }
                                $("#TOP_PRICE_" + (i + 1)).text(data[i].CurrentPrice);
                                $("#TOP_CHANGE_" + (i + 1)).text(data[i].Change + "  " + data[i].PercentChange + "%");
                            }
                        }
                    });
                }
                $(document).ready(function () {
                    getStockDataTop2();
                });
                setInterval(function () { getStockDataTop2(); }, 50000);
            </script>   
            <script type="text/javascript">
                var TTmaxpage = 0;
                var TTcurpage = 1;

                function NagivateTT(isleft) {
                    if (isleft) {
                        if (TTcurpage > 1) {
                            ShowTTPage(--TTcurpage);
                        }
                        else {
                            TTcurpage = TTmaxpage;
                            ShowTTPage(TTcurpage);
                        }
                    }
                    else {
                        if (TTcurpage < TTmaxpage) {
                            ShowTTPage(++TTcurpage);
                        }
                        else {
                            TTcurpage = 1;
                            ShowTTPage(TTcurpage);
                        }
                    }
                }
                function ShowTTPage(pageno) {
                    $("#divLastest div:visible").hide();
                    $("#divLastest_" + pageno).show();
                }

                $(document).ready(function () {
                    TTmaxpage = 3 - $("#divLastest div.none").length;
                });
    </script>   
