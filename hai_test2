<script type="text/javascript">
    $().ready(function () {
        $('#stockSearch').autocomplete('/vsSearchBox.ashx',
        {
            autoFill: false,
            selectFirst: true,
            matchContains: true,
            width: 400,
            scroll: false,
            delay: 400,
            minChars: 1,
            max: 10,
            slideUp: false,
            extraParams: { lmrk: 3 },
            
            language: 'en',
            
            formatItem: function (row, i, max) {
                return row[0] + " " + row[1];
            },
            formatMatch: function (row, i, max) {
                return row[0] + " " + row[1];
            },
            formatResult: function (row) {
                return row[0];
            }

        }).result(function (event, row, formatted) {
            if (row) {
                var sPath = window.location.href;
                if (sPath.indexOf("/profile") > -1
                    || sPath.indexOf("/transaction-statistics") > -1 || sPath.indexOf("/technical-analysis") > -1
                    || sPath.indexOf("/financials") > -1 || sPath.indexOf("/news-event") > -1 
                    || sPath.indexOf("/documents") > -1 || sPath.indexOf("/related-companies") > -1
                    || sPath.indexOf("/ownership-structure") > -1 || sPath.indexOf("/subsidiaries-and-associates") > -1
                    || sPath.indexOf("/board-of-management") > -1 || sPath.indexOf("/labor-structure") > -1) {
                    var comp = "/AAM/";
                    var idx = sPath.indexOf(comp);
                    location.href = sPath.replace(sPath.substr(idx, comp.length), "/" + $(this).val() + "/");
                }
                else {
                    location.href = row[2].replace("http://finance.vietstock.vn/","/");
                }
            }
        });

        //Dành cho so sánh chart
        $("#company1,#company2,#company3,#BFR_GetMore").autocomplete('/vsSearchBox.ashx', {
            autoFill: false,
            selectFirst: true,
            matchContains: true,
            width: 400,
            scroll: false,
            delay: 400,
            minChars: 1,
            max: 10,
            slideUp: true,
            extraParams: { lmrk: 2 },
            
            language: 'en',
            
            formatItem: function (row, i, max) {
                return row[0] + " " + row[1].toString();
            },
            formatMatch: function (row, i, max) {
                return row[0] + " " + row[1].toString();
            },
            formatResult: function (row) {
                return row[0].toString();
            }
        });
    });

    function locdau(str) {
        str = str.replace("  ", " ");
        str = str.replace(" - ", " ");
        str = str.replace(" & ", " ");
        str = str.toLowerCase();
        str = str.replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g, "a");
        str = str.replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g, "e");  
        str= str.replace(/ì|í|ị|ỉ|ĩ/g,"i");  
        str= str.replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g,"o");  
        str= str.replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g,"u");  
        str= str.replace(/ỳ|ý|ỵ|ỷ|ỹ/g,"y");  
        str= str.replace(/đ/g,"d");  
        str= str.replace(/!|@|%|\^|\*|\(|\)|\+|\=|\<|\>|\?|\/|,|\.|\:|\;|\'| |\"|\&|\#|\[|\]|~|$|_/g,"-"); 
        /* tìm và thay thế các kí tự đặc biệt trong chuỗi sang kí tự - */ 
        str= str.replace(/-+-/g,"-"); //thay thế 2- thành 1- 
        str = str.replace(/^\-+|\-+$/g, "");
        //cắt bỏ ký tự - ở đầu và cuối chuỗi     
        str = escape(str);
        str = str.replace(/%u0300|%u0301|%u0303|%u0309|%u0323/g, "");
        return str;  
    }  
           
</script>
<input type="text" id="stockSearch" style="width:100%"/>


