<script type="text/javascript">
     var containerRel;
      $(document).ready(function(){           
        var $tabs = $('#container_StockRelative').tabs({ fx: { height: 'toggle', opacity: 'toggle' },
            spinner: 'Loading...', 
            load: function(e,ui) {
                    var selected = $tabs.tabs('option', 'selected');
                    if(selected == 0)
                    {
                        $('#contain0').append($('#CompNoPart'));
                    }
                    else if(selected == 1)
                    {
                        $('#contain1').append($('#CompNoPart'));
                    }
                    else if(selected == 2)
                    {
                        $('#contain2').append($('#CompNoPart'));
                    }
                    else if(selected == 3)
                    {
                        $('#contain3').append($('#CompNoPart'));
                    }
            },
            select: function(e, ui) {
                url_select = $.data(ui.tab, 'load.tabs');
                return true;
            },
            selected: 0,
            ajaxOptions: {
				error: function(XMLHttpRequest, textStatus, errorThrown) {
					//alert("Couldn't load this tab. We'll try to fix this as soon as possible. If this wouldn't be a demo.");
				},
				data: {},
                success: function(data, textStatus) {
                    switch($tabs.tabs('option', 'selected'))
                    {
                        case 1:
                            stockRelativeTab(2);
                            break;
                        case 2:
                            stockRelativeTab(5);
                            break;
                        case 3:
                            stockRelativeTab(3);
                            break;
                        default:
                            stockRelativeTab(1);
                            break;
                    }
                    
                }
			}
         });
          
        var isDisplayYearTab = "False";
        $('#container_StockFinance').tabs({ 
            fx: { height: 'toggle', opacity: 'toggle' },
            spinner: 'Loading...', 
            selected: isDisplayYearTab == "True" ? 1 : 0,
            select: function(ui) {
                 $("#spNext1, #spPrevious1, #spLast1, #spFirst1").hide();           
                 $("#spNextYear1, #spPreviousYear1, #spLastYear1, #spFirstYear1").hide();
                 $("#spNextPlan1, #spPreviousPlan1, #spLastPlan1, #spFirstPlan1").hide();
            },
            ajaxOptions: {
				error: function(XMLHttpRequest, textStatus, errorThrown) {
					//alert("Couldn't load this tab. We'll try to fix this as soon as possible. If this wouldn't be a demo.");
				},
				data: {},
                success: function(data, textStatus) {
                }
			}  
        });
        
        $('#spNext1').click(function() { $('#spNext').click(); });             
        $('#spPrevious1').click(function() { $('#spPrevious').click(); });            
        $('#spNextYear1').click(function() { $('#spNextYear').click(); });            
        $('#spPreviousYear1').click(function() { $('#spPreviousYear').click(); });
        $('#spNextPlan1').click(function() { $('#spNextPlan').click(); });            
        $('#spPreviousPlan1').click(function() { $('#spPreviousPlan').click(); });
        
        $('#spLast1').click(function() { $('#spLast').click(); });
        $('#spFirst1').click(function() { $('#spFirst').click(); });
        $('#spLastYear1').click(function() { $('#spLastYear').click(); });
        $('#spFirstYear1').click(function() { $('#spFirstYear').click(); });
        $('#spLastPlan1').click(function() { $('#spLastPlan').click(); });
        $('#spFirstPlan1').click(function() { $('#spFirstPlan').click(); });

        if($("#table596").length == 0)
            $(".StockEOD_Lite_Visible").hide();
    });
 
    function addCommas(nStr)
    {
        nStr += '';
        x = nStr.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }   
    
    function stockRelativeTab(catid)   
    {
        $('#container' + catid).tabs({ fx: { height: 'toggle', opacity: 'toggle' },spinner: 'Loading...',
            load: function(e, ui) {
                url_select = $.data(ui.tab, 'load.tabs');
                if(!($(ui.panel).attr("id").indexOf("All") > -1))
                    containerRel = "#" + $(ui.panel).attr("id");
            },
            ajaxOptions: {
			    error: function(XMLHttpRequest, textStatus, errorThrown) {
				    //alert("Couldn't load this tab. We'll try to fix this as soon as possible. If this wouldn't be a demo.");
			    },
			    data: {},
                success: function(data, textStatus) {
                    if($('#container' + catid).tabs('option', 'selected') == 1)
                    {           
                        stockRelativeTabPaging(catid)
                    }
                }
		    }
        });
    }
    
    function stockRelativeTabPaging(catid)   
    {
        $("#pagerLinks").remove();
        $("#content" + catid).tablePager({ 
            offset: 0, //where to start
		    limit: 5, // how many rows each page
		    totalItems: $("#spCompNo").text(), //total row
		    previousText: "<",
		    nextText: ">",
		    url: '/Controls/TradingResult/StockRelativeAll.aspx',
		    data: "language=en&scode=AAM&catid=" + catid + "&page=",
		    sliderSize: 5,
		    curPage: 1,
		    callback:function(url) {
		        url_select = url;
		    }
        });
        
        containerRel = "#content" + catid;
    }
</script>


