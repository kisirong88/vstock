<script type="text/javascript">
    var CurrentPage = 1;
    $(document).ready(function () {
        if(2 == 1)
            $("#ViewYear, #ViewYear_B").removeClass("ViewReportTab").addClass("ViewReportTabSelected");
        else
            $("#ViewQuarter, #ViewQuarter_B").removeClass("ViewReportTab").addClass("ViewReportTabSelected");
        GetReport('BCTQ');

        BindTermClickEvent();
    });

    var GetReport = function(ReportType) {
        $.ajax({
            url: "/Controls/Report/Data/GetBCTQ.ashx",
            context: document.body,
            data: {
                rptType: ReportType,
                scode: 'AAM',
                bizType: '1',
                rptUnit: '1000000',
                rptTermTypeID: $(".ViewReportTabSelected").attr("id").replace("_B","") == "ViewYear" ? 1 : 2,
                page: CurrentPage
            },
            beforeSend: function () {
                $(".BR_modTable > div").html("&nbsp;<br /><img src='/Themes/Default/Images/loading.gif' style='height: 30px;' id='imgLoading'/>");
            },
            success: function (content) {
                $(".BR_modTable > div").html(content);
                              
                Paging();
                BindTermClickEvent();
                CreateChart();
            }
        });
    }

    function BindTermClickEvent()
    {
        $(".ViewReportTabSelected").unbind();
        $(".ViewReportTab").unbind().click(function () {
            var idTab = $(this).attr("id").replace("_B","");
            $(".ViewReportTabSelected").removeClass("ViewReportTabSelected").addClass("ViewReportTab");
            $("#" + idTab + ",#" + idTab + "_B").removeClass("ViewReportTab").addClass("ViewReportTabSelected");
            CurrentPage = 1;
            if(idTab != "ViewPlan")
                GetReport('BCTQ');
            else
                GetReport('CTKH');
        });
    }

    function Paging()
    {
        var TotalRow = $("#Tc_TotalRow").length > 0 ? $("#Tc_TotalRow").text() : 0;
        var TotalPage = 0;
        var selectID = "BCTQ";
        if($(".ViewReportTabSelected").attr("id") == "ViewPlan")
            selectID = "CTKH";
         
        $("#UnGoFirst, #UnGoPrev, #GoFirst, #GoPrev, #GoNext, #GoLast, #UnGoNext, #UnGoLast").hide().unbind();
        $("#UnGoFirst_B, #UnGoPrev_B, #GoFirst_B, #GoPrev_B, #GoNext_B, #GoLast_B, #UnGoNext_B, #UnGoLast_B").hide().unbind();
            
        if(TotalRow % 4 == 0)
            TotalPage = parseInt(TotalRow/4);
        else
            TotalPage = parseInt(TotalRow/4) + 1;

        
        if(CurrentPage == 1)
        {
            if(TotalPage > 1)
            {
                $("#UnGoFirst, #UnGoPrev, #GoNext, #GoLast").show();
                $("#UnGoFirst_B, #UnGoPrev_B, #GoNext_B, #GoLast_B").show();
            }
        }
        else if(CurrentPage == TotalPage)
        {
            $("#GoFirst, #GoPrev, #UnGoNext, #UnGoLast").show();
            $("#GoFirst_B, #GoPrev_B, #UnGoNext_B, #UnGoLast_B").show();
        }
        else
        {
            $("#GoFirst, #GoPrev, #GoNext, #GoLast").show();
            $("#GoFirst_B, #GoPrev_B, #GoNext_B, #GoLast_B").show();
        }

        $("#GoPrev,#GoPrev_B").click(function(){
            CurrentPage--;
            GetReport(selectID);
        });

        $("#GoNext,#GoNext_B").click(function(){
            CurrentPage++;
            GetReport(selectID);
        });

        $("#GoFirst,#GoFirst_B").click(function(){
            CurrentPage = 1;
            GetReport(selectID);
        });

        $("#GoLast,#GoLast_B").click(function(){
            CurrentPage = TotalPage;
            GetReport(selectID);
        }); 

    }

    function CreateChart(){
        $(".FFree_Grid .rpt_chart").each(function(index) {
            var chart_data = $(this).text().split(",");
            var color = "8CADC6";
            var color1 = "|8CADC6";

            if('False' && 'AAM' == 'BCG')
            {
                color = "3E7D24";
                color1 = "|3E7D24";
            }
            else if('False' && 'AAM' == 'DGW')
            {
                color = "D16402";
                color1 = "|D16402";
            }

            for (i = 1; i < chart_data.length; i++)
            {
                if (chart_data[i] != "-" && chart_data[i - 1] != "-" && parseFloat(chart_data[i]) < parseFloat(chart_data[i - 1])) 
                    color += color1;
                else if (chart_data[i] != "-" && chart_data[i - 1] != "-" && parseFloat(chart_data[i]) == parseFloat(chart_data[i - 1])) 
                    color += color1;
                else
                    color += color1;
            }
            $(this).before("<img src='http://" + (index % 9) + ".chart.apis.google.com/chart?cht=bvg&chs=35x17&chd=t:" + $(this).text() + "&chds=a&chco=" + color + "&chbh=a,0,3&chxt=x,y&chxs=0,000000,0,0,_,000000|1,000000,0,0,_,000000&chma=0,0,2,0'/>");
        });
        
//        $(".FFree_Grid .rpt_chart").each(function(index) {
//            var chart_data = $(this).text().split(",");
//            var color = "008000";
//            for (i = 1; i < chart_data.length; i++)
//            {
//                if (chart_data[i] != "-" && chart_data[i - 1] != "-" && parseFloat(chart_data[i]) < parseFloat(chart_data[i - 1])) 
//                    color += "|E60000";
//                else if (chart_data[i] != "-" && chart_data[i - 1] != "-" && parseFloat(chart_data[i]) == parseFloat(chart_data[i - 1])) 
//                    color += "|FF8C00";
//                else
//                    color += "|008000";
//            }
//            $(this).before("<img src='http://" + (index % 9) + ".chart.apis.google.com/chart?cht=bvg&chs=35x17&chd=t:" + $(this).text() + "&chds=a&chco=" + color + "&chbh=a,0,3&chxt=x,y&chxs=0,000000,0,0,_,000000|1,000000,0,0,_,000000&chma=0,0,2,0'/>");
//        });
    }

</script>
                    </td>
                </tr>
            </table>
