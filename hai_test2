<script type="text/javascript">
    var oldprice = 0;
    var oldvol = 0;
    var oldbuyred = 0;
    var oldsellred = 0;
    var oldforeignbuyvol = 0;
    var url_select;
    var t;
    var MktSattus = 1;
    $().ready(function() {
        //oldprice = parseInt($("#close").text().replace(/,/g,''));
        var d = new Date();
        var gio = d.getHours();
        var phut = d.getMinutes();
        var thu = d.getDay();
        
        
        
        $('#chart1D,#chart1W,#chart3M,#chart6M,#chart1Y').click(function(){
            var timechart = this.id.substring(5);
            $('#choseChart span').removeClass().addClass('chartChoose');
            $(this).removeClass().addClass('chartChooseSeleted');
            $('#stkChart').attr("src","http://chart.vietstock.vn/finance/AAM" + timechart + ".png?_=" + Math.random());
        });
        
          
            getRealTimeData();
        
        //var sPath_Com = window.location.href;
        var comselect = 0;
        if($("#content").length > 0)
        {
            comselect = $("#content").offset().top;
            //var tabselect = $("#tabselect").offset().top;
            $("html:not(:animated),body:not(:animated)").animate({ scrollTop: comselect}, 100 );
        }
    }); 
    function checkTime(i)
    {
        if (i<10) 
        {
            i="0" + i;
        }
        return i;
    }
    function getRealTimeData()
    {
        $.ajax({
            //url: '/Controls/TradingResult/GetStockInfoData.ashx',
            url: '/AjaxData/TradingResult/GetStockData.ashx',
            type: "GET",
            cache: false,
            data: "scode=AAM",
            dataType: "json",
            success: function(data){                
                if(data != "")
                {
                    var date = eval(data[0].CloseDate.replace(/\/Date\((\d+)\)\//gi, "new Date($1)"));
                    dateVN = checkTime(date.getDate()) + "/" + checkTime(date.getMonth() + 1) + "/" + date.getFullYear();
                    dateEN = checkTime(date.getMonth() + 1) + "/" + checkTime(date.getDate()) + "/" + date.getFullYear();
                    time = checkTime(date.getHours()) + ":" + checkTime(date.getMinutes());
                   
                    if(data[0].MarketStatus == 0)
                    {
                        $("#date").html(dateEN + ", " + time + "<br/>Trading/Closed");

                        $('#choseChart span').removeClass().addClass('chartChoose');
                        $('#chart1W').removeClass().addClass('chartChooseSeleted');
                        $('#stkChart').attr("src","http://chart.vietstock.vn/finance/AAM1W.png?_=" + Math.random());
                    }
                    else
                    {
                        $("#date").html(dateEN + ", " + time + "<br/>Trading" );
                    }
                    
                    if(data[0].OpenPrice == null)
                        $("#open").text("-");
                    else
                        $("#open").text(addCommas(data[0].OpenPrice));
                    
                    if(data[0].Highest == null)
                        $("#high").text("-");
                    else    
                        $("#high").text(addCommas(data[0].Highest));
                    if(data[0].Lowest == null)
                        $("#low").text("-");
                    else   
                        $("#low").text(addCommas(data[0].Lowest));
                    if(data[0].TradingVolume == null)
                        $("#vol").text("-");
                    else    
                    {
                        if(oldvol != data[0].TradingVolume)
                        {
                            $("#vol").addClass("bgHightLight");
                            oldvol = data[0].TradingVolume;
                        }
                        $("#vol").text(addCommas(data[0].TradingVolume));
                    }
                        
                    if(data[0].CapitalLevel == null)
                        $("#capitallevel").text("-");
                    else
                        $("#capitallevel").text(addCommas(data[0].CapitalLevel));
                    if(data[0].PE == null)
                        $("#pe").text("-");
                    else    
                        $("#pe").text(addCommas(data[0].PE.toFixed(2)));
                    if(data[0].Dividend == null)
                        $("#dividend").text("-");
                    else
                        $("#dividend").text(addCommas(data[0].Dividend));
                    if(data[0].YearHigh == null)
                        $("#yearhigh").text("-");
                    else   
                        $("#yearhigh").text(addCommas(data[0].YearHigh));
                    if(data[0].FwPE == null)
                        $("#fpe").text("-");
                    else
                        $("#fpe").text(addCommas(data[0].FwPE.toFixed(2)));
                    if(data[0].Yield == null)
                        $("#yield").text("-");
                    else
                        $("#yield").text(addCommas(data[0].Yield.toFixed(2)));
                     if(data[0].YearLow == null)
                        $("#yearlow").text("-");
                    else   
                        $("#yearlow").text(addCommas(data[0].YearLow));
                    if(data[0].Beta == null)
                        $("#beta").text("-");
                    else
                        $("#beta").text(addCommas(data[0].Beta));
                        
                    if(data[0].TotalShares == null)
                        $("#share").text("-");
                    else
                        $("#share").text(addCommas(data[0].TotalShares));
                    if(data[0].AvgVol == null)
                        $("#avgvol").text("-");
                    else
                        $("#avgvol").text(addCommas(data[0].AvgVol));
                    if(data[0].EPS == null)
                        $("#eps").text("-");
                    else    
                        $("#eps").text(addCommas(data[0].EPS.toFixed(0)));
                    if(data[0].ForeignRoom == null)
                        $("#foreign").text("-");
                    else
                        $("#foreign").text(addCommas(data[0].ForeignRoom));
                    if(data[0].ForeignBuyVolume == null)
                        $("#foreignbuyvol").text("-");
                    else
                    {
                        if(oldforeignbuyvol != data[0].ForeignBuyVolume)
                        {
                            $("#foreignbuyvol").addClass("bgHightLight");
                            oldforeignbuyvol = data[0].ForeignBuyVolume;
                        }
                        $("#foreignbuyvol").text(addCommas(data[0].ForeignBuyVolume));    
                    }
                    if(data[0].PB == null)
                        $("#pb").text("-");
                    else
                        $("#pb").text(addCommas(data[0].PB.toFixed(2)));    
                    if(data[0].BVPS == null)
                        $("#bvps").text("-");
                    else
                        $("#bvps").text(addCommas(data[0].BVPS.toFixed(0)));  
                    if(data[0].BuyRedundancy == null)
                        $("#buyred").text("-");
                    else
                    {
                        if(oldbuyred != data[0].BuyRedundancy)
                        {
                            $("#buyred").addClass("bgHightLight");
                            oldbuyred = data[0].BuyRedundancy;
                        }
                        $("#buyred").text(addCommas(data[0].BuyRedundancy));
                    }
                    if(data[0].SellRedundancy == null)
                        $("#sellred").text("-");
                    else
                    {
                        if(oldsellred != data[0].SellRedundancy)
                        {
                            $("#sellred").addClass("bgHightLight");
                            oldsellred = data[0].SellRedundancy;
                        }
                        $("#sellred").text(addCommas(data[0].SellRedundancy));
                    }
                                    
                    var change = data[0].Oscillate; 
                    var changepercent = data[0].PercentOscillate;
                    var color = data[0].ColorId;
                    
                    if(data[0].ClosePrice == null)
                        $("#close").html("-");
                                
                    if(change == null)
                        $("#change").html("- (-%)")
                    else
                    {
                        if(oldprice != data[0].ClosePrice)
                        {
                            $("#close").addClass("bgHightLight");
                            oldprice = data[0].ClosePrice;
                        }
                        
                        switch(color)
                        {
                            case -2:
                                $("#close").html("<span class='textBLUE'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/San_03.png' width='15' height='20'/>");  
                                $("#change").html("<span class='textBLUE'>" + addCommas(change) + "</span><span class='textBLUE'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break;
                            case -1:
                                $("#close").html("<span class='textRED'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/Giam_03.png' width='15' height='20'/>");   
                                $("#change").html("<span class='textRED'>" + addCommas(change) + "</span><span class='textRED'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break;
                            case 0:
                                $("#close").html("<span class='textORANGE'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/DungGia_03.png' width='15' height='20'/>");   
                                $("#change").html("<span class='textORANGE'>" + addCommas(change) + "</span><span class='textORANGE'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break; 
                            case 1:
                                $("#close").html("<span class='textGREEN'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/Tang_03.png' width='15' height='20'/>");   
                                $("#change").html("<span class='textGREEN'>" + addCommas(change) + "</span><span class='textGREEN'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break; 
                            case 2:
                                $("#close").html("<span class='textPURPLE'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/Tran_03.png' width='15' height='20'/>");   
                                $("#change").html("<span class='textPURPLE'>" + addCommas(change) + "</span><span class='textPURPLE'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break;  
                            default:
                                $("#close").html("<span class='textBLACK'>" + addCommas(data[0].ClosePrice)+ "</span>&nbsp;&nbsp;<img src='/Themes/Default/Images/DungGia_03.png' width='15' height='20'/>");   
                                $("#change").html("<span class='textBLACK'>" + addCommas(change) + "</span><span class='textBLACK'>&nbsp;(" + changepercent.toFixed(2) + "%)</span>");
                                break;
                        }

                        $("#close").delayFunc(2000, function(){
                            $(this).removeClass("bgHightLight");
                        });
                        $("#vol").delayFunc(2000, function(){
                            $(this).removeClass("bgHightLight");
                        });
                        $("#foreignbuyvol").delayFunc(2000, function(){
                            $(this).removeClass("bgHightLight");
                        });
                        $("#buyred").delayFunc(2000, function(){
                            $(this).removeClass("bgHightLight");
                        });
                        $("#sellred").delayFunc(2000, function(){
                            $(this).removeClass("bgHightLight");
                        });
                    }

                    MktSattus = data[0].MarketStatus;
                    if(MktSattus != 0)
                        var t = setTimeout("getRealTimeData()",10000);
                }
                
                /*Lay hinh anh realtime*/
                if($('#stkChart').attr("src").indexOf("http://chart.vietstock.vn/finance/AAM1D.png") > -1)
                    $('#stkChart').attr("src","http://chart.vietstock.vn/finance/AAM1D.png?_=" + Math.random());

                
            }
        });
    }
    
    $.fn.delayFunc = function(time, callback){
        // Empty function:
        jQuery.fx.step.delay = function(){};
        // Return meaningless animation, (will be added to queue)
        return this.animate({delay:1}, time, callback);
    }
</script>

<script type="text/javascript">
    var curIndex = "HSX_Idx";
    var oldPriceArray = [];
    var oldVolArray = [];
    $(document).ready(function(){
        $('#myslidemenu').css("background-color","white");
        $('#IndPart').css("display","none");
        $('#rdStockCode').attr("checked","checked");
        $('#rdIndList').click( function() { 
            $('#StockPart').css("display","none");
            $('#IndPart').css("display","block");
            $('#rdIndList').attr("checked","checked");
            $('#rdStockCode').removeAttr("checked"); 
        }); 
        $('#rdStockCode').click( function() { 
            $('#StockPart').css("display","block");
            $('#IndPart').css("display","none");
            $('#rdIndList').removeAttr("checked");
            $('#rdStockCode').attr("checked","checked"); 
        });
                
        getIndexRealTimeData();
        //changeIndex();

        if ("True" == "False") {
            $("#table292").hide();
            alert("Mã này không tồn tại trong hệ thống");
            history.back();
        }

        setTimeout(function () {
            /*Like Facebook*/
            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=314194798653804";
                fjs.parentNode.insertBefore(js, fjs);
            } (document, 'script', 'facebook-jssdk'));

            /*Google Plus*/
            window.___gcfg = { lang: 'en' };

            (function () {
                var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                po.src = 'https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
            })();
        }, 1000);

        $("#add-to-neo").click(function () {
            Add_TheoDoiCK("AAM", 1);
            alert("Bạn đã thêm vào Danh sách theo dõi thành công. Cảm ơn!");

            $(".neo_show_nav").removeClass("neo_show_nav");
            $(".theodoimck").parent().addClass("neo_show_nav");

            $('.neo_show_tab').hide();
            $('#theodoimck').show();
            $.cookie(COOKIE_NAME_NEOStatus, content_show, options);
        });

        $("#add-to-porforio").click(function () {
            Add_DanhMucCK("AAM");
            alert("Bạn đã thêm vào Danh mục thành công. Cảm ơn!");
        });

    });

    function Add_DanhMucCK(stockCode) {
        var COOKIE_DanhMucCK = $.cookie("NEO_DanhMucCK_Cookies");
        if (COOKIE_DanhMucCK == null) {
            COOKIE_DanhMucCK = "";
        }
        if (COOKIE_DanhMucCK.toLowerCase().indexOf(stockCode.toLowerCase()) == -1) {
            COOKIE_DanhMucCK = COOKIE_DanhMucCK + stockCode + "|";
            $.cookie("NEO_DanhMucCK_Cookies", COOKIE_DanhMucCK.toLowerCase(), options);
        }        
    }

    function getIndexRealTimeData()
    {
        $.ajax({
            url: '/Controls/TradingResult/GetMarketInfoData.ashx',
            type: "GET",
            cache: false,
            dataType: "json",
            success: function (data) {
                /*if (data != "") {
                    if (data[0].PerChange > 0)
                        $("#HSX_Idx").html(data[0].CloseIndex.toFixed(2) + "&nbsp;&nbsp;<span style='color: Green;'>" + data[0].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[0].PerChange).toFixed(2) + "%</span>");
                    else if (data[0].PerChange < 0)
                        $("#HSX_Idx").html(data[0].CloseIndex.toFixed(2) + "&nbsp;&nbsp;<span style='color: Red;'>" + data[0].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[0].PerChange).toFixed(2) + "%</span>");
                    else if (data[0].PerChange == 0)
                        $("#HSX_Idx").html(data[0].CloseIndex + "&nbsp;&nbsp;<span style='color: Orange;'>" + data[0].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[0].PerChange).toFixed(2) + "%</span>");

                    if (data[1].PerChange > 0)
                        $("#HNX_Idx").html(data[1].CloseIndex.toFixed(2) + "&nbsp;&nbsp;<span style='color: Green;'>" + data[1].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[1].PerChange).toFixed(2) + "%</span>");
                    else if (data[1].PerChange < 0)
                        $("#HNX_Idx").html(data[1].CloseIndex.toFixed(2) + "&nbsp;&nbsp;<span style='color: Red;'>" + data[1].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[1].PerChange).toFixed(2) + "%</span>");
                    else if (data[1].PerChange == 0)
                        $("#HNX_Idx").html(data[1].CloseIndex + "&nbsp;&nbsp;<span style='color: Orange;'>" + data[1].Change.toFixed(2) + "&nbsp;&nbsp;" + (data[1].PerChange).toFixed(2) + "%</span>");

                }*/
                $("td").removeClass("StockRelPrice");
                $("td").removeClass("StockRelVol");

                if (url_select != undefined) {
                    $.ajax({
                        url: url_select,
                        success: function (data) {
                            if (data.indexOf("FFree_Grid") > -1) {
                                $(containerRel).html(data);

                                var objPrice = $('.StockRelPrice');
                                var arrPrice = $.makeArray(objPrice);
                                if (oldPriceArray.length != 0) {
                                    $.each(oldPriceArray, function (index, value) {
                                        $.each(arrPrice, function (index_new, value_new) {
                                            if (index == index_new && $(value).text() != $(value_new).text()) {
                                                $(".StockRelPrice:eq(" + index + ")").attr("name", $(this).css("color")).css("color", "white").addClass("bgHightLight");
                                            }
                                        });
                                    });
                                }
                                oldPriceArray = arrPrice;

                                var objVol = $('.StockRelVol');
                                var arrVol = $.makeArray(objVol);
                                if (oldVolArray.length != 0) {
                                    $.each(oldVolArray, function (index, value) {
                                        $.each(arrVol, function (index_new, value_new) {
                                            if (index == index_new && $(value).text() != $(value_new).text()) {
                                                $(".StockRelVol:eq(" + index + ")").addClass("bgHightLight");
                                            }
                                        });
                                    });
                                }
                                oldVolArray = arrVol;


                                $(".StockRelPrice").delayFunc(2000, function () {
                                    $(this).removeClass("bgHightLight").css("color", $(this).attr("name"));
                                });
                                $(".StockRelVol").delayFunc(2000, function () {
                                    $(this).removeClass("bgHightLight");
                                });

                                if (MktSattus != 0)
                                    setTimeout("getIndexRealTimeData()", 15000);
                            }
                            else {
                                if (MktSattus != 0)
                                    setTimeout("getIndexRealTimeData()", 3000);
                            }

                        }
                    });
                }
                else {
                    if (MktSattus != 0)
                        setTimeout("getIndexRealTimeData()", 3000);
                }

            }
        }); 
    }
    
    function changeIndex()
    {
        $("#HSX_Idx,#HNX_Idx").fadeOut("slow").fadeIn("slow");
        setTimeout('changeIndex()', 5000);
    }
</script>
