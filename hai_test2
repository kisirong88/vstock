<script	type="text/javascript">
function displayTabProfile(tabname)
{
    var tabs = new Array('tab1', 'tab2', 'tab3', 'tab4', 'tab5','tab6');
	for (i = 0; i < tabs.length; i++)
	{
		document.getElementById(tabs[i]).style.display = 'none';
		document.getElementById('s' + tabs[i]).className = 'FFree_Grid_Tab';
	}
	
	document.getElementById(tabname).style.display = 'block';
	document.getElementById('s' + tabname).className = 'FFree_Grid_TabSelected';
}

function showContentTip(obj) {
    if ($(obj).children(":last").text().length < 110)
        $(".qtip").hide();
}    


$(document).ready(function () {
    $(".FFull_EventCalendar_Title").each(function () {
        var content = $(this).find(".more_content").html();
        var title = $(this).find(".more_title").html();
        content = content.replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&amp;nbsp;/g, " ");
        title = title.replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&amp;nbsp;/g, " ");
        $(this).qtip({
            content: {
                text: content,
                title: {
                    text: title,
                    button: '&nbsp;'
                }
            },
            position: {
                corner: {
                    target: "bottomLeft",
                    tooltip: 'topLeft'
                }, adjust: { x: 0, y: 0 }
            },
            api: {
                onShow: function () {
                    showContentTip(this.elements.target);
                }
            },
            show: { when: 'click', solo: true },
            hide: 'unfocus',
            style: {
                padding: 5,
                background: '#f1f9fe',
                color: 'black',
                textAlign: 'left',
                border: {
                    width: 2,
                    radius: 1,
                    color: '#2ba4f4'
                },
                title: {
                    background: '#A9DAFA',
                    'padding-right': '5px'
                },
                classes: {
                    content: 'tootipcontent'
                },
                width: 401,
                button: { 'background': 'url(/Themes/Default/Images/close_dark.png) no-repeat left top', 'width': '24px', 'height': '24px' }

            }

        });
    });
 });
</script>

