            </td>
        </tr>
    </table> 
<script type="text/javascript" >
    $(document).ready(function () {
        //Xoa li cuoi cung trong menu
        $('#menu_top li:last').remove();

        $('#IndPart').css("display", "none");
        $('#rdStockCode').attr("checked", "checked");
        $('#rdIndList').click(function () {
            $('#StockPart').css("display", "none");
            $('#IndPart').css("display", "block");
            $('#rdIndList').attr("checked", "checked");
            $('#rdStockCode').removeAttr("checked");
        });
        $('#rdStockCode').click(function () {
            $('#StockPart').css("display", "block");
            $('#IndPart').css("display", "none");
            $('#rdIndList').removeAttr("checked");
            $('#rdStockCode').attr("checked", "checked");
        });

        $('#tableBoardOfManagements .prev').each(function () {
            var content = $(this).children().children(".more_content").html();
            $(this).qtip({
                content: content,
                position: {
                    corner: {
                        target: 'leftMiddle',
                        tooltip: 'rightMiddle'
                    }
                },
                show: 'mouseover',
                style: {
                    padding: 5,
                    background: 'white',
                    color: 'black',
                    textAlign: 'left',
                    border: {
                        width: 1,
                        radius: 1,
                        color: '#0261C4'
                    },
                    tip: 'rightMiddle',
                    name: 'blue',
                    classes: {
                        content: 'tootipcontent'
                    }
                }
            });
        });
    });
</script>



                                
                            
