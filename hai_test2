<script type="text/javascript">
    $(document).ready(function () {
        $('#textfield2').autocomplete("/vsSearchBox.ashx",
        {
            autoFill: false,
            selectFirst: false,
            matchContains: true,
            width: 400,
            scroll: false,
            delay: 400,
            minChars: 1,
            max: 10,
            canEnter: true,
            formatItem: function (row, i, max) {
                return row[0] + " " + row[1];
            },
            formatMatch: function (row, i, max) {
                return row[0] + " " + row[1];
            },
            formatResult: function (row) {
                return row[0];
            }

        }).result(function (event, row, formatted) {
            if (row)
                location.href = row[2];
            else
                window.open('http://vietstock.vn/Search.aspx?key=' + $(this).val(), '_blank');
        });


        $("#aCophieu").click(function () {
            search(1);
        });
        $("#aTinbai").click(function () {
            search(2);
        });


        $('.main_menu a[rel=menu_71]').first().addClass("active4");
        $('#menu_71').show();
        var sPath = window.location.href;
        if(sPath === "http://finance.vietstock.vn" || sPath === "http://finance.vietstock.vn/") {
            $('.main_menu a[class=fi_home]').addClass('fi_active');
        }
        else if (sPath.indexOf("tong-quan-thi-truong") > -1) {
            $('.main_menu a[rel=menu_71_72]').addClass('fi_active');
        }
        else if (sPath.indexOf("bo-loc") > -1) {
            $('.main_menu a[rel=menu_71_74]').addClass('fi_active');
        }
        else if (sPath.indexOf("so-sanh-co-phieu") > -1) {
            $('.main_menu a[rel=menu_71_75]').addClass('fi_active');
        }
        else if (sPath.indexOf("lich-su-kien") > -1) {
            $('.main_menu a[rel=menu_71_73]').addClass('fi_active');
        }
        else if (sPath.indexOf("SeasonReport.aspx") > -1 || sPath.indexOf("tai-lieu-co-dong") > -1 || sPath.indexOf("/tai-lieu/") > -1) {
            $('.main_menu a[rel=menu_71_77]').addClass('fi_active');
        }
        else if (sPath.indexOf("ty-le-ky-quy") > -1) {
            $('.main_menu a[rel=menu_71_94]').addClass('fi_active');
        }
        else if (sPath.indexOf("tam-ly-thi-truong") > -1) {
            $('.main_menu a[rel=menu_71_101]').addClass('fi_active');
        }
        else if (sPath.indexOf("nien-giam") > -1) {
            $('.main_menu a[rel=menu_71_102]').addClass('fi_active');
        }
        else {
            $('.main_menu a[rel=menu_71_78]').addClass('fi_active');
        }

        setTimeout(function () {
            $.get("/Handler.ashx?u=http://common.vietstock.vn/Header_Footer/Ajax/GetHeaderFooter.ashx?typedo=indexnews", function(result){
                $("#cms_indexnews").html(result); 

                $('a.tab').click(function () {
                    $('.activeD').removeClass('activeD');
                    $(this).addClass('activeD');
                    $('.newsright_item').hide();
                    var content_show = $(this).attr('rel');
                    $('#' + content_show).show();

                });

                getIndexQuote();
            });            
        }, 3000);

    });
    function clearText(thefield) {
        if (thefield.defaultValue == thefield.value)
            thefield.value = ""
    }
    function search(rdo) {
        var txtSearch = document.getElementById('textfield2');
        if (txtSearch.value != "" && txtSearch.value != "Nhập MCK, từ khóa") {
            if (rdo == 1) window.open('http://vietstock.vn/Search.aspx?type=1&key=' + txtSearch.value, '_blank');
            else window.location.href = 'http://vietstock.vn/Search.aspx?type=2&key=' + txtSearch.value;
        }
        return false;
    }

    function getIndexQuote() {
        $.getJSON(
            //'/Handler.ashx?u=http://ajax.vietstock.vn/GetChart.ashx',
            'http://ajax.vietstock.vn/GetChart.ashx',
            function (data) {
                $.each(data, function (i, item) {

                    if (item.Change > 0) { sColor = "green"; }
                    else if (item.Change < 0) { sColor = "red"; }
                    else { sColor = "yellow"; }
                    $("#PRICE_" + item.Symbol).html("<span class='txt_index'>" + item.CurrentPrice + "</span>");
                    $("#CHANGE1_" + item.Symbol).html("<span class='txt_" + sColor + "'>" + item.Change + "</span>");
                    $("#CHANGE_" + item.Symbol).attr("class", "point_" + sColor);
                    $("#PERCENT_" + item.Symbol).attr("class", "txt_" + sColor);
                    $("#PERCENT_" + item.Symbol).text(item.PercentChange + "%");
                });
                setTimeout("getIndexQuote()", 50000);
            }
        );
    }
</script>

